{
  "name": "@movie-platform/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:prod": "playwright test --config=tests/e2e-production/playwright.production.config.ts",
    "test:e2e:prod:smoke": "playwright test --config=tests/e2e-production/playwright.production.config.ts --project=smoke",
    "test:e2e:prod:auth": "playwright test --config=tests/e2e-production/playwright.production.config.ts --project=auth",
    "test:e2e:prod:report": "playwright show-report tests/e2e-production/reports/html",
    "clean": "rm -rf .next .turbo node_modules"
  },
